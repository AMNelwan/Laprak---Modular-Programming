def cek_digit_belakang(a, b, c):
  """
  Mengecek apakah minimal dua dari tiga parameter memiliki digit paling kanan yang sama.

  Args:
    a: Bilangan pertama.
    b: Bilangan kedua.
    c: Bilangan ketiga.

  Returns:
    True jika minimal dua digit paling kanan sama, False jika tidak.
  """

  # Mendapatkan digit paling kanan
  digit_a = a % 10
  digit_b = b % 10
  digit_c = c % 10

  # Menghitung jumlah digit yang sama
  sama = 1 if digit_a == digit_b else 0
  sama += 1 if digit_a == digit_c else 0
  sama += 1 if digit_b == digit_c else 0

  # Mengembalikan True jika minimal dua digit sama
  return sama >= 2

# Membaca input dari pengguna
a = int(input("Masukkan bilangan pertama: "))
b = int(input("Masukkan bilangan kedua: "))
c = int(input("Masukkan bilangan ketiga: "))

# Cek digit paling kanan
hasil = cek_digit_belakang(a, b, c)

# Menampilkan hasil
if hasil:
  print("Minimal dua dari tiga bilangan memiliki digit paling kanan yang sama.")
else:
  print("Tidak ada dua bilangan yang memiliki digit paling kanan yang sama.")

# Contoh test case
print("\nContoh test case:")
test_cases = [
  (30, 20, 18),
  (145, 5, 100),
  (71, 187, 18),
  (1024, 14, 94),
  (53, 8900, 658),
]

for case in test_cases:
  a, b, c = case
  hasil = cek_digit_belakang(a, b, c)
  print(f"Input: {a}, {b}, {c} - Output: {hasil}")
